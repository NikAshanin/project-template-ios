name: {{name}}
options:
  createIntermediateGroups: true
  xcodeVersion: 9.2
  deploymentTarget:
    iOS: "10.0"
configs:
  StandardDebug: debug
  StandardRelease: release
  EnterpriseDebug: debug
  EnterpriseRelease: release
targets:
  {{name}}:
    type: application
    platform: iOS
    sources:
      - path: {{name}}
        buildPhase: sources
      - path: {{name}}/Analytics
        buildPhase: sources
      - path: {{name}}/Cells
        buildPhase: sources
      - path: {{name}}/Controllers
        buildPhase: sources
      - path: {{name}}/Extensions
        buildPhase: sources
      - path: {{name}}/Generated
        buildPhase: sources
      - path: {{name}}/Models
        buildPhase: sources
      - path: {{name}}/Networking
        buildPhase: sources
      - path: {{name}}/Protocols
        buildPhase: sources
      - path: {{name}}/Realm
        buildPhase: sources
      - path: {{name}}/Resources
        buildPhase: sources
      - path: {{name}}/Services
        buildPhase: sources
      - path: {{name}}/Views
        buildPhase: sources
